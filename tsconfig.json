{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES6",
		"outDir": "out",
		"rootDir": "src",

		"strict": true,
		"strictNullChecks": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true
	}
}
